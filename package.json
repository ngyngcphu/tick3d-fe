{
  "name": "react-template",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "start": "yarn barrels && cross-env NODE_ENV=development vite --host",
    "build": "tsc && cross-env NODE_ENV=production vite build",
    "preview": "vite preview --host",
    "barrels": "barrelsby --config .barrelsby.json -q",
    "lint": "eslint '**/*.{tsx,ts,js}'",
    "format": "prettier '**/*.{tsx,ts,js,json,md,yml,yaml}' --write",
    "prepare": "is-ci || husky install && yarn api:pull",
    "api:pull": "openapi-typescript https://tick3d-server.tickflow.net/docs/json -o src/openapi-spec.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.2.0",
    "@material-tailwind/react": "^2.1.0",
    "axios": "^1.4.0",
    "moment": "^2.29.4",
    "openapi-fetch": "^0.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-icons": "^4.11.0",
    "react-router-dom": "^6.18.0",
    "react-toastify": "^9.1.3",
    "yup": "^1.2.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "barrelsby": "^2.8.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^14.0.0",
    "openapi-typescript": "^7.0.0-next.4",
    "postcss": "^8.4.27",
    "prettier": "3.0.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
